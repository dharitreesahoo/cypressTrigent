{
  "name": "frontend-cypress",
  "version": "1.0.0",
  "description": "Front End Cypress E2E Tests",
  "scripts": {
    "lint": "eslint -c .eslintrc.json . --ext .ts",
    "compile": "tsc --build tsconfig.json",
    "cy:open": "cypress open",
    "cy:run:headless": "cypress run --headless --browser chrome",
    "cy:chrome:headless": "cypress run --headless --browser chrome -s ${NPM_ARGS}",
    "cy:run:selected": "cypress run --headless --browser chrome --env grep=${NPM_ARGS}",
    "cy:login": "cypress run --headless --browser chrome --spec \"cypress/tests/spiceworks-login-spec.ts\"",
    "cy:quote": "cypress run --env grep=quote",
    "delete:reports": "rm cypress/results/junit/*",
    "merge-reports": "junit-merge -d cypress/results/junit -o cypress/results/junit/merged.xml",
    "combine-reports": "mochawesome-merge cypress/results/json/*.json -o cypress/results/json/mochawesome-merged.json",
    "generate-report": "marge cypress/results/json/mochawesome-merged.json -f report -o cypress/results/json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tridrive/auto-cypress-ts-tests"
  },
  "keywords": [
    "Cypress",
    "Trigent"
  ],
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/tridrive/auto-cypress-ts-tests/issues"
  },
  "homepage": "https://github.com/tridrive/auto-cypress-ts-tests#readme",
  "devDependencies": {
    "@testing-library/cypress": "^7.0.2",
    "@types/node": "^13.13.0",
    "@types/pdf-parse": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "8.0.6",
    "cypress": "^7.7.0",
    "cypress-audit": "^1.0.0",
    "cypress-failed-log": "^2.9.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-grep": "^2.5.2",
    "cypress-junit-reporter": "^1.3.1",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-select-typescript-tests": "^1.1.2",
    "cypress-terminal-report": "^2.4.0",
    "cypress-xpath": "^1.6.1",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "ts-loader": "^7.0.2",
    "tslint": "^6.1.3",
    "tslint-plugin-cypress": "^1.0.4",
    "typescript": "^4.1.2",
    "webpack": "^4.43.0",
    "ws": "^7.5.2"
  },
  "dependencies": {
    "@types/faker": "^5.1.5",
    "autoset-status-cypress-testrail-reporter": "^1.1.2",
    "cypress-downloadfile": "^1.2.0",
    "cypress-testrail-reporter": "^1.2.8",
    "faker": "^5.1.0",
    "junit-merge": "^2.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "pdf-parse": "^1.1.1",
    "typescript-string-operations": "^1.4.1",
    "xunit-viewer": "^7.1.2"
  }
}
